<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="de.bht.fpa.mail.s820789.fsnavigation.NavigationView"
            id="de.bht.fpa.mail.s820789.fsnavigation.view2"
            name="FS Navigation"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               id="de.bht.fpa.mail.s820789.fsnavigation.view2"
               minimized="false"
               relationship="left"
               relative="org.eclipse.ui.editorss">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="de.bht.fpa.mail.s820789.fsnavigation.commands.category"
            name="Category">
      </category>
      <command
            categoryId="de.bht.fpa.mail.s820789.fsnavigation.commands.category"
            id="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand"
            name="Change Base Directory">
      </command>
      <command
            categoryId="de.bht.fpa.mail.s820789.fsnavigation.commands.category"
            id="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand1"
            name="History">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.bht.fpa.mail.s820789.fsnavigation.handlers.SetBaseDirHandler"
            commandId="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand">
      </handler>
      <handler
            class="de.bht.fpa.mail.s820789.fsnavigation.handlers.HistoryHandler"
            commandId="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="de.bht.fpa.mail.s820789.fsnavigation.menus.sampleMenu"
               label="FS Navigation"
               mnemonic="M">
            <command
                  commandId="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand"
                  id="de.bht.fpa.mail.s820789.fsnavigation.menus.sampleCommand"
                  mnemonic="S">
            </command>
            <command
                  commandId="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand1"
                  id="de.bht.fpa.mail.s820789.fsnavigation.menus.sampleCommand1"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="de.bht.fpa.mail.s820789.fsnavigation.toolbars.sampleToolbar">
            <command
                  commandId="de.bht.fpa.mail.s820789.fsnavigation.commands.sampleCommand"
                  icon="icons/sample.gif"
                  id="de.bht.fpa.mail.s820789.fsnavigation.toolbars.sampleCommand"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
